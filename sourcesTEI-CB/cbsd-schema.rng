            </oneOrMore>
         </group>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.declaring.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_att.global.facs.attributes">
      <ref name="tei_att.global.facs.attribute.facs"/>
   </define>
   <define name="tei_att.global.facs.attribute.facs">
      <optional>
         <attribute name="facs">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (fac-similé) pointe directement vers une image ou vers une partie
        d'une image correspondant au contenu de l'élément.</a:documentation>
            <list>
               <ref name="tei_data.pointer"/>
